{
  "compilerOptions": {
    // "allowImportingTsExtensions": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "scripthost"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": ["./src/**/*", "public/assets/data/game_objects.js", "public/assets/data/continents.js",]
}
