{
  "version": "0.1.0",
  "homepage": "https://alunosuff.github.io/trabalho-gerencia-projetos/",
  "type": "module",
  "scripts": {
    "dev:server": "nodemon dist/server/server.js",
    "start": "nodemon server.js",
    "build:dev:client": "vite build -w --ssrManifest --outDir dist/client",
    "build:dev:server": "vite build -w --ssr src/server/server.ts --outDir dist/server",
    "serve": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/server/server.ts --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server"
  },
  "dependencies": {
    "express": "^4.18.2",
    "phaser": "3.55.2",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^9.0.2",
    "gh-pages": "^5.0.0",
    "nodemon": "^2.0.22",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
