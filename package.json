{
  "name": "war",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "nodemon dist/server/server.js",
    "dev": "concurrently --kill-others \"npm run build:dev:client\" \"npm run build:dev:server\" \"npm run dev:server\"",
    "build:dev:client": "vite build -w --ssrManifest --outDir dist/client",
    "build:dev:server": "vite build -w --ssr src/server/server.ts --outDir dist/server",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/server/server.ts --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "serve": "NODE_ENV=production node dist/server/server.js",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "express": "^4.18.2",
    "phaser": "3.55.2",
    "phaser3-rex-plugins": "^1.60.2",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^9.0.2",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "phaser3-rex-plugins": "^1.60.2"
  }
}
